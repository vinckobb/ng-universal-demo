{
    "name": "ng-universal-demo",
    "version": "1.0.0",
    "description": "appDescription",
    "scripts":
    {
        "build": "webpack --env.dll && webpack --env.ssr",
        "build:watch": "webpack --env.ssr && webpack -w --env.dll",
        "build:min": "webpack --env.aot --env.prod && webpack --env.aot --env.ssr",
        "build:gulp": "gulp build",
        "build:gulp:aot": "gulp build:aot",
        "prebuild": "npm run clean && npm run build:gulp && npm run build:prepare:dev",
        "prebuild:watch": "npm run prebuild",
        "prebuild:min": "npm run clean && npm run build:gulp:aot",
        "postbuild:min": "npm run clean:tmp",
        "build:prepare:dev": "webpack --config webpack.config.dev.js",
        "clean": "rimraf wwwroot && npm run clean:tmp",
        "clean:tmp": "rimraf app.aot",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "app-config:w": "gulp watch-config",
        "scss": "gulp compile-scss",
        "tsc": "tsc",
        "update-versions": "nrs versionDeployment.config.json",
        "server": "node server.js",
        "start": "npm run build:gulp && npm run build:prepare:dev && concurrent \"npm run app-config:w\" \"npm run server -- --webpack\""
    },
    "engines":
    {
        "node": ">=6.0.0"
    },
    "author": "appAuthor",
    "license": "MIT",
